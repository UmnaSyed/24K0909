#include<stdio.h>
int main(){
int sum1, sum2;
sum1=0;
sum2=0;
int arr[2][3][3]={{{2,3,4},{1,2,5},{2,6,7}},{{9,4,5},{4,6,7},{6,9,3}}};
for (int i=0; i<3; i++){
    for (int j = 0; j<3; j++){
        sum1=sum1+arr[0][i][j];
    }
}
for (int i=0; i<3; i++){
    for (int j = 0; j<3; j++){
        sum2=sum2+arr[1][i][j];
    }
}
printf("The sum of numbers on the first page is: %d", sum1);
printf("The sum of numbers on the second page is: %d", sum2);
return 0;
}


