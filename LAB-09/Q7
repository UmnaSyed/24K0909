#include<stdio.h>
void prime(int num1, int a){
    a=0;
    for (int i=2; i<num1; i++){
        if (num1%i==0){
            a++;
        }
        }
        if (a==0){
            printf("The number you entered is a prime number.");
        }
        else {
            printf("The number you entered is a composite number.");
    
    }
   
}
int main (){
    int num1, a;
    printf("Enter your number");
    scanf("%d", &num1);
    if (num1==1 || num1==0){
            printf("Your number is neither prime nor composite");
            return 0;
    }
    
    prime(num1, a);
return 0;
}
