#include<stdio.h>
void swap(int num1, int num2){
    num1 = num1 ^ num2;
    num2 = num1 ^ num2;
    num1 = num1 ^ num2;
    printf("the swapped integers are:\n number 1: %d\n number 2: %d", num1, num2);
}
int main (){
    int num1, num2;
    printf("Enter the two numbers you want to swap\n");
    scanf("%d %d", &num1, &num2);
    
    swap(num1, num2);
return 0;
}
