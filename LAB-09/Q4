#include<stdio.h>
#include<string.h>
int main (){
char arr[4][9]={"umna","afsah", "huzaifa", "khuzaima"};
char userarr[15];
printf("Enter the names you want to find: \n");
scanf(" %s", &userarr);
int result=0;
for (int i=0; i<4; i++){
if (strcmp(arr[i], userarr)==0){
   result++; 
   break;
}
}
if (result==1){
    printf("FOUND!");
}
else {
    printf("NOT FOUND!");
}
return 0;
}

