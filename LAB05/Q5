#include <stdio.h>
int main() {
int num1, num2;
 printf("Enter your first number: ");
 scanf("%d", &num1);
 printf("Enter your second number: ");
 scanf("%d", &num2);

num1>num2 ? printf("First number is greater than the second number.\n"): printf("Second number is greater than the first number.\n");
return 0;
}

